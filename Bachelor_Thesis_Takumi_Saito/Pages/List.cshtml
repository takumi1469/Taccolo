@page
@model Bachelor_Thesis_Takumi_Saito.Pages.ListModel

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>List</title>
</head>
<body>

<h4>LearningSet</h4>
<hr />
<section class="list">
        <ul>
            @foreach (var learningSet in Model.LearningSets)
            {
                <li>
                    <a asp-page="/Set" asp-route-id="@learningSet.Id"> @learningSet.Id @learningSet.Title </a> <!-- display the input text or any relevant property -->
                </li>
                
            }
        </ul>
</section>

<div>
    <a asp-page="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
</body>
</html>
